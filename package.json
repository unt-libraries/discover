{
  "name": "unt-blacklight",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "func-names": [
        "error",
        "never"
      ],
      "func-style": [
        "off"
      ],
      "import/no-extraneous-dependencies": [
        "off"
      ]
    },
    "env": {
      "browser": true,
      "es6": true,
      "jquery": true,
      "serviceworker": true
    }
  }
}
