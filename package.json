{
  "name": "discover",
  "version": "1.2.0",
  "scripts": {
    "start": "docker compose up",
    "stop": "docker compose down",
    "build": "docker compose build",
    "web:run": "docker compose run --rm web",
    "rubyStyle": "docker compose run --rm web rubocop --require rubocop-airbnb",
    "javascriptStyle": "docker compose run --rm web node_modules/eslint/bin/eslint.js .",
    "web:compileAssets": "docker compose run -e \"RAILS_ENV=production\" -e \"NODE_OPTIONS=--openssl-legacy-provider\" --rm web rails assets:precompile",
    "web:dbCreate": "docker compose run --rm web rake db:create",
    "web:dbMigrate": "docker compose run --rm web rake db:migrate",
    "web:credentials": "docker compose run --rm web bash -c \"EDITOR=nano rails credentials:edit\"",
    "test:compileAssets": "docker compose run -e \"RAILS_ENV=test\" -e \"NODE_OPTIONS=--openssl-legacy-provider\" --rm web rails assets:precompile",
    "test:jest": "docker compose run -e \"RAILS_ENV=test\" -e \"NODE_ENV=test\" --rm web yarn jest",
    "test:rspec": "docker compose run -e \"RAILS_ENV=test\" -e \"NODE_ENV=test\" -e \"NODE_OPTIONS=--openssl-legacy-provider\" --rm web bundle exec rspec",
    "test:verbose": "yarn test --format documentation",
    "tailLogs": "docker compose logs -t --tail",
    "followLogs": "docker compose logs -t --follow",
    "grepLogs": "docker compose logs -t | grep",
    "docker:stopAll": "docker stop $(docker ps -a -q)",
    "docker:removeAll": "docker rm $(docker ps -a -q)",
    "docker:prune": "docker system prune -a && docker volume prune -a"
  },
  "private": true,
  "dependencies": {
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@babel/runtime-corejs3": "^7.26.0",
    "@popperjs/core": "^2.11.5",
    "@rails/ujs": "^6.1.710",
    "@rails/webpacker": "5.4.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/bootstrap": "^5.2.6",
    "@types/browser-update": "^3.3.0",
    "@types/jest": "^29.5.0",
    "@types/mark.js": "^8.11.8",
    "acorn": "8.14.1",
    "babel-jest": "^29.5.0",
    "babel-loader": "8.4.1",
    "blacklight-frontend": "^7.38.0",
    "blacklight-range-limit": "8.5.0",
    "bootstrap": "5.3.3",
    "browser-update": "^3.3.55",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "6.1.2",
    "core-js": "^3.42.0",
    "css-minimizer-webpack-plugin": "1.3.0",
    "dayjs": "^1.11.13",
    "fork-ts-checker-webpack-plugin": "6.5.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "lazysizes": "^5.3.1",
    "mark.js": "^8.11.1",
    "moment": "^2.30.1",
    "moment-locales-webpack-plugin": "^1.1.2",
    "rails-erb-loader": "^5.5.2",
    "sass-loader": "10.5.2",
    "source-map-loader": "1.1.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "8.4.0",
    "turbolinks": "^5.2.0",
    "typescript": "^5.8.3",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.26.0",
    "expose-loader": "1",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3"
  },
  "resolutions": {
    "acorn": "^8.14.1",
    "terser-webpack-plugin": "^4.2.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
