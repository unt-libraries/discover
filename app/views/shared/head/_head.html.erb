<head>
  <meta charset="utf-8">
  <meta name="google" content="notranslate">

  <!-- Internet Explorer use the highest version available -->
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title><%= render_page_title %></title>

  <meta name="description" content="Discover searches the UNT Libraries' descriptive records of 3 million+ items, including books, media, music (and more).">

  <%= opensearch_description_tag application_name, opensearch_catalog_url(format: 'xml') %>

  <!-- CSS -->
  <!-- Webfonts -->
  <!-- Preconnect for assets -->
  <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin>
  <link href="https://lgapi-us.libapps.com" rel="preconnect" crossorigin>
  <link href="https://books.google.com" rel="preconnect" crossorigin>

  <%= preload_link_tag "https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,400;0,800;1,400&family=Oswald:wght@300;400&subset=latin-ext&display=swap", as: "style" %>
  <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,400;0,800;1,400&family=Oswald:wght@300;400&subset=latin-ext&display=swap"
        media="print"
        onload="this.media='all'">
  <noscript>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,400;0,800;1,400&family=Oswald:wght@300;400&subset=latin-ext&display=swap">
  </noscript>

  <%= stylesheet_packs_with_chunks_tag "priority", "base", media: "all", 'data-turbolinks-track': 'reload' %>

  <!--[if IE 9]>
  <link href="https://library.unt.edu/assets/vendor/bootstrap-ie8/css/bootstrap-ie9.css?v=4.3.1" rel="stylesheet">
  <link rel="stylesheet" href="https://library.unt.edu/assets/styles/ie.min.css">
  <![endif]-->
  <!--[if lte IE 8]>
  <link href="https://library.unt.edu/assets/vendor/bootstrap-ie8/css/bootstrap-ie8.css?v=4.3.1" rel="stylesheet">
  <script src="https://library.unt.edu/assets/vendor/html5shiv/dist/html5shiv.min.js?v=3.7.3"></script>
  <link rel="stylesheet" href="https://library.unt.edu/assets/styles/ie.min.css">
  <![endif]-->

  <%= render '/shared/head/icons' %>

  <%= render '/shared/head/opengraph' %>

  <!-- Head scripts -->
  <%= javascript_packs_with_chunks_tag 'priority', 'data-turbolinks-track': 'reload', async: true %>
  <%= javascript_packs_with_chunks_tag 'analytics', 'data-turbolinks-track': 'reload', async: true %>
  <script src="https://kit.fontawesome.com/5f309fc1bc.js" crossorigin="anonymous" async="async" data-mutate-approach="sync"></script>

  <%= content_for :head_scripts %>
  <% content_for(:head_scripts) do %>
    <%= javascript_packs_with_chunks_tag 'base', 'data-turbolinks-track': 'reload', defer: true %>
  <% end %>

  <%= csrf_meta_tags %>
  <%= content_for(:head) %>

  <%= render '/shared/head/chat-widget' %>
</head>