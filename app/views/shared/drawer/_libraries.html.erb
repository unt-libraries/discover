<% url_sycamore = "#{url_home}/sycamore/" %>
<% url_discovery_park = "#{url_home}/discovery-park/" %>
<% url_media = "#{url_home}/media/" %>
<% url_graduate = "#{url_home}/services/for-graduate-students/" %>
<% url_faculty = "#{url_home}/services/for-faculty/" %>
<% url_community = "#{url_home}/services/for-community-visitors/" %>
<% url_accessibility = "#{url_home}/services/persons-with-disabilities/" %>
<% url_spaces = "#{url_home}/spaces/" %>
