           <a href="<%= url_sycamore %>#maps"
              rel="noopener"
              class="nav-link d-inline icon-link small"
              ga-on="click"
              ga-event-category="link - drawer"
              ga-event-action="libraries"
              ga-event-label="<%= url_sycamore %>#maps"
              ga-event-value="1"><i title="" class="fas fa-map-marker-alt fa-lg"></i>
              <span class="sr-only">Sycamore Library Location and Floor Maps</span>
           </a>
