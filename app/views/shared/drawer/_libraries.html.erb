<div class="drawer fade" id="libraries-drawer" tabindex="-1" role="dialog" aria-labelledby="libraries-drawer-title" aria-hidden="true" data-turbolinks="false">
  <div class="modal-dialog modal-dialog--right" role="document">
    <div class="drawer-content">
      <div class="drawer-header">
        <h2 class="drawer-title px-3" id="libraries-drawer-title">People &amp; Places</h2>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <i aria-hidden="true" class="fas fa-times fa-lg"></i>
        </button>
      </div>
      <div class="drawer-body">


        <h2 class="drawer__sub-heading px-3">Information For<span aria-hidden="true">...</span><span class="sr-only"> Specific Audiences</span></h2>

        <a href="<%= url_graduate %>"
           rel="noopener"
           class="nav-item nav-link"
           ga-on="click"
           ga-event-category="link - drawer"
           ga-event-action="libraries"
           ga-event-label="<%= url_graduate %>"
           ga-event-value="1"><span class="sr-only">Info for </span>Graduate Students</a>

        <a href="<%= url_faculty %>"
           rel="noopener"
           class="nav-item nav-link"
           ga-on="click"
           ga-event-category="link - drawer"
           ga-event-action="libraries"
           ga-event-label="<%= url_faculty %>"
           ga-event-value="1"><span class="sr-only">Info for </span>Faculty/Staff</a>

        <a href="<%= url_community %>"
           rel="noopener"
           class="nav-item nav-link"
           ga-on="click"
           ga-event-category="link - drawer"
           ga-event-action="libraries"
           ga-event-label="<%= url_community %>"
           ga-event-value="1"><span class="sr-only">Info for </span>Community &amp; Visitors</a>

        <a href="<%= url_accessibility %>"
           rel="noopener"
           class="nav-item nav-link"
           ga-on="click"
           ga-event-category="link - drawer"
           ga-event-action="libraries"
           ga-event-label="<%= url_accessibility %>"
           ga-event-value="1"><span class="sr-only">Info for </span>Persons with Disabilities</a>

        <hr class="drawer__divider" />

        <h2 class="drawer__sub-heading px-3">Locations &amp; Hours</h2>

        <div class="nav-item d-flex">
          <a href="<%= url_willis %>"
             rel="noopener"
             class="nav-link col"
             ga-on="click"
             ga-event-category="link - drawer"
             ga-event-action="libraries"
             ga-event-label="<%= url_willis %>"
             ga-event-value="1">Willis Library</a>

          <span class="float-right mr-3">
           <a href="<%= url_willis %>#hours"
              rel="noopener"
              class="nav-link d-inline icon-link small"
              ga-on="click"
              ga-event-category="link - drawer"
              ga-event-action="libraries"
              ga-event-label="<%= url_willis %>#hours"
              ga-event-value="1"><i aria-hidden="true" class="fa fa-calendar-alt fa-lg"></i>
             <span class="sr-only">Willis Library Hours</span>
           </a> &nbsp;

           <a href="<%= url_willis %>#maps"
              rel="noopener"
              class="nav-link d-inline icon-link small"
              ga-on="click"
              ga-event-category="link - drawer"
              ga-event-action="libraries"
              ga-event-label="<%= url_willis %>#maps"
              ga-event-value="1"><i aria-hidden="true" class="fas fa-map-marker-alt fa-lg"></i>
              <span class="sr-only">Willis Library Location and Floor Maps</span>
